<core:FragmentDefinition xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:ux="sap.uxap"
    xmlns:smartVariant="sap.ui.comp.smartvariants"
    xmlns:smartfilterbar="sap.ui.comp.smartfilterbar"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <!-- <Text text = "ello"/> -->
    <smartVariant:SmartVariantManagement id="idICDSVM" entitySet="ICDMASTER" useFavorites="true" showShare="true" showExecuteOnSelection="true" />
    <smartfilterbar:SmartFilterBar id="idICDSFB" entitySet="ICDMASTER" smartVariant="idICDSVM" persistencyKey="ICDSFB_key" filterChange="onFilterChange" />
    <smartTable:SmartTable id="idICD" entitySet="ICDMASTER" tableType="ResponsiveTable" useVariantManagement="false" useTablePersonalisation="true" enableAutoBinding="true" enableExport="false" header="" class="sapUiResponsiveContentPadding sapUxAPObjectPageSubSectionAlignContent"  persistencyKey="ICDSFB_key" app:useSmartField="true" initiallyVisibleFields="Sitecode,Sitename,ChapterID,ChapterIDDescription,Level2,Level2Description,Level3,Level3Description,Level4,Level4Description">
        <smartTable:customToolbar>
            <Toolbar>
                <ToolbarSpacer />
                <Button icon="sap-icon://add" visible="{uiModel>/editable}" press="onAddICD" />
            </Toolbar>
        </smartTable:customToolbar>
        <Table id="idtbICD" itemPress="ICDDetPress" mode="{= (${uiModel>/editable} ? 'Delete' : 'None')}" delete="onDeleteTableRow">
            <items>
                <ColumnListItem type="Active"/>
            </items>
        </Table>
    </smartTable:SmartTable>
</core:FragmentDefinition>